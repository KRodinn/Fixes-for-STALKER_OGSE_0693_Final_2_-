;-----------------------------------------------------------------------------------
;Сменные аддоны ОГСЕ
;-----------------------------------------------------------------------------------
;управляемые В СЕКЦИЯХ АДДОНОВ кости (64=54+b+5s+2m+2d) АК102 с привязкой by k01jan:
;wpn_body		патрон в стволе
;  wpn_grenade_launcher 
;Имеются посадочные места для установки штык-ножа.
;	wpn_grenade			
;  wpn_silencer	ТГП-В
;  wpn_scope	- (движковая, д/б пустая)
;  mag			- пустая, анимированная
;  	ammo		2 патрона в магазине (скрытА при разряжании, виднА при заряжании)
;  	mag: m1		СВД 10
;  	mag: m2		СВУ-А 20
;  m3			- 
;  muzzle		- 
;  d1			щека
;  d2			калоша
;  s1			ПСО-1 х4
;  s2			1П59 "Гиперон" (пер.кратность)
;  s3			1ПН51 (НСПУ-3 "Казуар") х3,46
;  s4			1П29 (УСП-1 "Тюльпан") х4
;  s5			ЭКП-8М-01 "Кобра" х1
;  safe (сделать предохранитель)
;-----------------------------------------------------------------------------------
;материал: МИР объект-large_weapon
;шейдеры:
;lens_scope1 непрозрачная линза acog - ХУД модель-weapons / МИР модель-weapons
;collimator_metka0 первая линза Кобры - прозрачная с меткой (сменная) ХУД худ-reddotsight1 / МИР 
;collimator_lens01 вторая линза Кобры - полупрозрачная круглая зелёная ХУД модель-transparent / МИР 
;-----------------------------------------------------------------------------------


; базовая секция
[wpn_swd_base]:wpn_swd
silencer_status			= 2
grenade_launcher_status	= 0
scope_status			= 2
scope_mount_status		= 0
grip_status				= 2
magazine_status			= 2
script_binding  = ogse_bind_weapon.init
;скрытые (управляемые) кости: d2, d1, s1, s2, s3, s4, s5, m1, m2, m3, muzzle
;СВД нет щеки приклада - d1 надо ВСЕГДА скрывать
;СВД нет калоши приклада - d2 надо ВСЕГДА скрывать
managed_bones = d2, d1, s1, s2, s3, s4, s5, m1, m2, m3, muzzle
;не надо своего описания description				= enc_weapons1_wpn-swd
;inv_name	        	= wpn-swd
;inv_name_short			= wpn-swd_s
;cost					= 7500
hud = wpn_swd_base_hud

[wpn_swd_base_hud]:wpn_swd_hud



