      <font font="letterica16" r="255" g="255" b="255"/>
    </descr_text>

    <tab x="10" y="170" width="247" height="26">
      <button x="0" y="0" width="116" height="26">
        <texture_offset x="0" y="-7"/>
        <texture>ui_button_tablist</texture>
        <text font="graffiti19" align="c">Рюкзак</text>
        <text_color>
          <e r="238" g="153" b="26"/>
          <t r="238" g="153" b="26"/>
          <d r="238" g="153" b="26"/>
          <h r="238" g="153" b="26"/>
        </text_color>
      </button>
      <button x="126" y="0" width="116" height="26">
        <texture_offset x="0" y="-7"/>
        <texture>ui_button_tablist</texture>
        <text font="graffiti19" align="c">Инвентарь</text>
        <text_color>
          <e r="238" g="153" b="26"/>
          <t r="238" g="153" b="26"/>
          <d r="238" g="153" b="26"/>
          <h r="238" g="153" b="26"/>
        </text_color>
      </button>
