#include "common.h"

struct _in
{
	float4 pos		:TEXCOORD0;
	float4 tc		:TEXCOORD1;
};
struct _out
{
	float4 depth:COLOR0;
	float4 color:COLOR1;
	float4 tc:COLOR2;
};
uniform float4 c_color;
uniform float4 c_params;
uniform sampler2D s_jitter1;
uniform sampler2D s_jitter2;
uniform sampler2D s_jitter5;
_out main(_in I)
{
	_out o;
	float4 P = I.pos;
	P.xy = float2(1.0,1.0) - I.pos.xy;
	o.depth = float4(saturate(I.pos.z), 0.0, c_params.x, 0.0/*c_params.y*/);
	o.color = c_color;
//	o.color.w = c_params.z;
	o.tc = I.tc;
//	o.tc.w = c_params.w;
	return o;
};